{
	"name": "mark-img",
	"version": "0.0.3",
	"type": "module",
	"keywords": [
		"图片标注",
		"图像标注",
		"annotation",
		"image annotation",
		"visualization",
		"machine learning",
		"深度学习",
		"javascript",
		"image mark",
		"image-mark",
		"mark-img",
		"mark img",
		"mark image",
		"img mark",
		"img-mark"
	],
	"description": "This JS library is a framework-agnostic image annotation tool that supports customization through a plugin system. It allows developers to integrate and extend functionality easily without altering the core library code.",
	"scripts": {
		"dev": "vite --mode development --force",
		"build": "vite build -c vite.lib.config.ts",
		"build-web": "pnpm run build-demo && pnpm run docs:build",
		"build-demo": "vite build --mode dev",
		"preview": "vite preview",
		"deploy:dev": "pnpm run build-demo && node ./node_modules/bbl-scripts/scripts/deploy.js dev",
		"docs:dev": "vitepress dev docs --port 6003",
		"docs:build": "vitepress build docs --outDir docs-dist",
		"docs:preview": "vitepress preview docs"
	},
	"repository": {
		"url": "git+https://github.com/hxdyj/image-mark.git"
	},
	"author": "hxdyj",
	"license": "MIT",
	"devDependencies": {
		"@arco-design/web-react": "^2.64.1",
		"@arco-plugins/vite-react": "^1.3.3",
		"@rollup/plugin-strip": "^3.0.4",
		"@types/gl-matrix": "^3.2.0",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.14.9",
		"@types/nprogress": "^0.2.3",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"@vitejs/plugin-react": "^4.3.3",
		"autoprefixer": "^10.4.19",
		"bbl-scripts": "^0.0.2",
		"classnames": "^2.5.1",
		"eslint": "^9.20.1",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.14",
		"less": "^4.2.0",
		"lodash-es": "^4.17.21",
		"nprogress": "^0.2.0",
		"postcss": "^8.5.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.27.0",
		"sass": "^1.77.6",
